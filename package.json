{
  "author": "Situm Technologies",
  "name": "@situm/sdk-js",
  "version": "0.8.0",
  "description": "A Javascript client to Situm services, supporting Node and browser.",
  "repository": "https://github.com/situmtech/situm-sdk-js",
  "main": "dist/cjs/situm-sdk.js",
  "module": "dist/es5/situm-sdk.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "situm",
    "rest",
    "sdk",
    "api",
    "library"
  ],
  "files": [
    "dist",
    "!dist/dts",
    "docs",
    "examples",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "SECURITY-POLICY.md"
  ],
  "scripts": {
    "build:watch": "rollup -c -w;",
    "build": "rimraf dist/* && rollup -c;",
    "cov": "run-s build; jest --coverage  && open-cli coverage/index.html",
    "doc:html": "typedoc",
    "doc:json": "typedoc --json",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "fix:lint": "eslint src --ext .ts --fix",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix": "run-s fix:*",
    "precommit": "lint-staged",
    "prepare-release": "run-s reset-hard test build doc:html",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell --no-summary --no-progress --no-must-find-files \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "jest --coverage",
    "test": "run-p test:*",
    "version": "standard-version"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "codecov": "^3.8.3",
    "colors": "^1.4.0",
    "commitizen": "^4.3.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "cspell": "^8.10.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.2.7",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^8.0.0",
    "prettier": "^3.3.2",
    "replace-in-file": "^8.1.0",
    "rimraf": "^5.0.9",
    "rollup": "2.74.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^2.9.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "run-s": "^0.0.0",
    "semantic-release": "^24.0.0",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.26.3",
    "typedoc-plugin-extras": "^3.1.0",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typedoc-plugin-rename-defaults": "^0.7.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {},
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
