  "favicon": "https://situm.com/wp-content/uploads/2020/06/cropped-logo-situm-180x180.png",
  "plugin": [
    "typedoc-plugin-extras",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults",
    "typedoc-github-theme"
  ]
}
