{
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "docs/public",
  "tsconfig": "tsconfig.json",
  "excludePrivate": false,
  // "excludeProtected": true,
  "excludeExternals": false,
  "hideGenerator": true,
  "readme": "./README.md",
